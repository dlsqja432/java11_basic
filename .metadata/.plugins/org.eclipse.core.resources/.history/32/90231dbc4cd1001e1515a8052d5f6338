package string;

import java.util.Scanner;

public class Exa1 {

	public static void main(String[] args) {

		Scanner scan = new Scanner(System.in);
		String s = scan.next();
		int total = 0;
		
		for(int i=0; i<s.length(); i++) {
			char c = s.charAt(i);
			if(c >= 'A') {
				total = total + 3;
			}
			if(c >= 'D') {
				total = total + 4;
			}
			else if(c >= 'G') {
				total = total + 5;
			}
			else if(c >= 'J') {
				total = total + 6;
			}
			else if(c >= 'M') {
				total = total + 7;
			}
			else if(c >= 'P') {
				total = total + 8;
			}
			else if(c >= 'T') {
				total = total + 9;
			}
			else if(c >= 'W') {
				total = total + 10;
			}
		}
		
		System.out.println(total);
	}

}
