package array;

import java.util.Scanner;

public class Ex6 {

	public static void main(String[] args) {

		Scanner scan = new Scanner(System.in);
		int n = scan.nextInt();
		int m = scan.nextInt();
		int[] arr = new int[n];
		int tmp;
		
		for(int i=1; i<=n; i++) {
			arr[i] = i;
		}
		
		for(int i=0; i<m; i++) {
			int a = scan.nextInt();
			int b = scan.nextInt();
			tmp = arr[a];
			arr[a] = arr[b];
			arr[b] = tmp;
		}
		
		for(int result : arr) {
			System.out.println(result);
		}
		
		scan.close();
	}

}
