package string;

import java.util.Scanner;

public class Exa1 {

	public static void main(String[] args) {

		Scanner scan = new Scanner(System.in);
		String s = scan.next();
		int total = 0;
		
		for(int i=0; i<s.length(); i++) {
			char c = s.charAt(i);
			if(c == 'A' || c == 'B' || c == 'C') {
				total = total + 3;
			}
			else if(c == 'D' || c == 'E' || c == 'F') {
				total = total + 4;
			}
		}
	}

}
