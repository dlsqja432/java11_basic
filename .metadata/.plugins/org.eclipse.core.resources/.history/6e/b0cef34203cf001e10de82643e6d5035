package org.kh.java;

public class Hello2 {

	public static void main(String[] args) {

		System.out.println("장인범");
		
		
//		CPU : 2진수
//		권한, 작업 : 8진수
//		주소 : 16진수
		

//		변수는 기억장치의 이름을 의미한다.
//		* 기본형 변수 > 단일 기억 장치(primitive)
//		byte : 1byte
//		short : 2byte
//		integer : 4byte
//		long : 8byte
//		boolean : 1byte
//		float : 4byte
//		double : 8byte
//		char : 2byte
//		
//		* 참조형 변수 > 복수 기억 장소(reference)
//		list, map, set
		
		
//		* 상수명 입력 방법
//		final static PI = 3.14; > 상수명은 대문자로 사용
		
//		*변수명 입력 방법
//		ive_average_age : snake 방식
//		iveAverageAge : camel 방식
//		자바는 보통 camel 방식을 사용
		
	}

}
