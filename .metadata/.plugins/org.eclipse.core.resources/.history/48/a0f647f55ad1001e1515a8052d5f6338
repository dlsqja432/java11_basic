package org.kh.app2;

public class StarPrint {

	public static void main(String[] args) {

		for(int i=0; i<4; i++) {
			for(int j=0; j<6; j++) {
				System.out.print("*");
			}
			System.out.println();
		}
		System.out.println();
		
		for(int i=0; i<5; i++) {
			for(int j=0; j<i+1; j++) {
				System.out.print('*');
			}
			System.out.println();
		}
		
		System.out.println();
		
		for(int i=0; i<6; i++) {
			for(int j=0; j<6-i; j++) {
				System.out.print('*');
			}
			System.out.println();
		}
	}

}
