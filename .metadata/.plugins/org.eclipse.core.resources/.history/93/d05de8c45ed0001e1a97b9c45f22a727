package array;

import java.util.Scanner;

public class Ex1 {

	public static void main(String[] args) {
		
		Scanner scanner = new Scanner(System.in);
		
		int n = scanner.nextInt();
		int[] arr = new int[n];
		
		for(int i=0; i<n; i++) {
			int input = scanner.nextInt();
			arr[i] = input;
		}
		
		int v = scanner.nextInt();
		int count = 0;
		
		for(int i=0; i<n; i++) {
			if(arr[i] == v) {
				count++;
			}
		}
		System.out.println(count);
	}
}
