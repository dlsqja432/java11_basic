package string;

import java.util.Scanner;

public class Ex6 {

	public static void main(String[] args) {

		Scanner scan = new Scanner(System.in);
		String s = scan.next();
		int[] arr = new int[26];
		int[] sArr = new int[s.length()];
		
		for(int i=0; i<26; i++) {
			arr[i] = i;
		}
		
		for(int i=0; i<s.length(); i++) {
			sArr[i] = s.charAt(i)-'a';
		}
	}

}
